<menu id="global-menu" class="">
  <li id="global-menu-mymenu">
    <a>
      <span class="mii-icon">
        <img src="../src/DummyMii.png" alt="User Page" />
      </span>
      <span>User Page</span>
    </a>
  </li>
  <li id="global-menu-feed">
    <a>Activity Feed</a>
  </li>
  <li id="global-menu-community" class="">
    <a href="/communities">Communities</a>
  </li>
  <li id="global-menu-message">
    <a>Messages<span class="badge" style="display: none">0</span></a>
  </li>
  <li id="global-menu-news" class="">
    <a>Notifications<span class="badge" style="display: none">0</span></a>
  </li>
  <% if (exit === true) { %>
  <li id="global-menu-exit" class="">
    <a role="button" href="javascript:void(0)" onclick="exit();">Close</a>
  </li>
  <li id="global-menu-back" style="display: none">
    <a
      role="button"
      onclick="back();"
      href="javascript:void(0)"
      class="accesskey-B"
      >Back</a
    >
  </li>
  <% } else { %>
  <li id="global-menu-exit" class="" style="display: none">
    <a role="button" href="javascript:void(0)" onclick="exit();">Close</a>
  </li>
  <li id="global-menu-back" class="">
    <a
      role="button"
      onclick="back();"
      href="javascript:void(0)"
      class="accesskey-B"
      >Back</a
    >
  </li>
  <% } %>
</menu>
